        <joint name="0_r_shoulder_lift_joint" type="hinge" pos="0 0 0" axis="0 1 0" range="-0.5236 1.3963" damping="1.0" />

        <body name="0_r_upper_arm_roll_link" pos="0 -0.3 0">
