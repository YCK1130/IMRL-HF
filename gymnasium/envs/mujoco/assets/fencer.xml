                <body name="0_r_forearm_link" pos="0 0 0">
                  <geom name="0_fa" type="capsule" fromto="0 0 0 0.291 0 0" size="0.05" conaffinity="1"/>

                  <body name="0_r_wrist_flex_link" pos="0.321 0 0">
