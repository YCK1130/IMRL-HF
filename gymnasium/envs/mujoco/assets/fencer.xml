    <body name="0_y_slide_link" pos="0 -0.4 0">
    <geom name="0_sp0" type="capsule" rgba="0.6 0.6 0.6 1" fromto="-0.1 0 -0.4 0.1 0 -0.4" size="0.1" />
    <joint name="0_y_slide" type="slide" pos="0 0 -0.4" axis="1 0 0" range="-1.0 1.0" />
    <body name="0_r_shoulder_pan_link" pos="0 0 0">
      <geom name="0_e1" type="sphere" rgba="0.6 0.6 0.6 1" pos="-0.06 0.05 0.2" size="0.05" conaffinity="1"/>
