      <joint name="1_r_shoulder_pan_joint" type="hinge" pos="0 0 0" axis="0 0 1" range="-2.2854 1.714602" damping="1.0" />

      <body name="1_r_shoulder_lift_link" pos="0 0 0" axisangle="0 0 1 1.57079">
        <geom name="1_sl" type="capsule" fromto="0 -0.3 0 0 0.3 0" size="0.1" conaffinity="1"/>
        <joint name="1_r_shoulder_lift_joint" type="hinge" pos="0 0 0" axis="0 1 0" range="-0.5236 1.3963" damping="1.0" />

        <body name="1_r_upper_arm_roll_link" pos="0 -0.3 0">
          <geom name="1_uar" type="capsule" fromto="-0.1 0 0 0.1 0 0" size="0.02" conaffinity="1"/>
          <joint name="1_r_upper_arm_roll_joint" type="hinge" pos="0 0 0" axis="1 0 0" range="-1.5 1.7" damping="0.1" />

          <body name="1_r_upper_arm_link" pos="0 0 0">
