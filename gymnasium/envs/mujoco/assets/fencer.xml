      <joint name="0_r_shoulder_pan_joint" type="hinge" pos="0 0 0" axis="0 0 1" range="-2.2854 1.714602" damping="1.0" />

      <body name="0_r_shoulder_lift_link" pos="0 0 0" axisangle="0 0 1 1.57079">
