          <joint name="0_r_upper_arm_roll_joint" type="hinge" pos="0 0 0" axis="1 0 0" range="-1.5 1.7" damping="0.1" />

          <body name="0_r_upper_arm_link" pos="0 0 0">
            <geom name="0_ua" type="capsule" fromto="0 0 0 0.4 0 0" size="0.06" conaffinity="1"/>

            <body name="0_r_elbow_flex_link" pos="0.4 0 0">
